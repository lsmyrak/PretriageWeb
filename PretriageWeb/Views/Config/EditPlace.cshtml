@model Pretriage.ViewModel.UnitListItem

<form asp-action="EditPlace" method="post" class="form-horizontal">
    <div class="form-group">
        <label asp-for="Status" class="control-label">Nazwa</label>
        <input type="text" asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <button class="btn btn-block btn-primary" type="submit">Dodaj</button>

</form>